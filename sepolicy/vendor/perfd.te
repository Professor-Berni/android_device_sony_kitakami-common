type perfd, domain;
type perfd_exec, exec_type, file_type;

# Started by init
init_daemon_domain(perfd)

allow perfd cgroup:file read;
allow perfd default_prop:file r_file_perms;
allow perfd proc:file r_file_perms;
allow perfd sysfs:dir r_dir_perms;
allow perfd sysfs:file rw_file_perms;
allow perfd system_data_file:dir w_dir_perms;
allow perfd system_data_file:file create_file_perms;
